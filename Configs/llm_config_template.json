{
    "query_type": "openai",
    "reasoning_mode": false,

    "azure_api_type": "azure",
    "azure_api_base": "https://your-azure-endpoint.openai.azure.com/",
    "azure_api_model": "gpt-4.1",
    "azure_api_version": "2025-01-01-preview",
    "azure_api_key": "your-azure-api-key",

    "openai_api_model": "gpt-4.1-mini",
    "openai_api_key": "your-openai-api-key",

    "ollama_model": "qwen3-coder:30b",
    "ollama_host": "127.0.0.1",
    "ollama_port": 11434,
    "ollama_context_size": 65536,

    "custom_openai_api_model": "your-custom-model",
    "custom_openai_api_key": "your-custom-api-key",
    "custom_openai_api_base": "http://your-custom-api-base/v1",

    "embedding_backend": "openai",
    "embedding_model": "text-embedding-3-large",
    "embedding_dimension": 3072,
    "embedding_openai_api_base": "https://api.openai.com/v1",
    "embedding_custom_api_key": "your-embedding-api-key",
    "embedding_custom_api_base": "http://your-embedding-api-base/v1",

    "max_attempts": 5,
    "max_token_length": 8000,
    "retry_wait_time": 10,
    "retry_wait_increment": 5,

    "default_max_tokens": 8000,
    "default_temperature": 0,
    "default_top_p": 0.3,
    "default_frequency_penalty": 0,
    "default_presence_penalty": 0,
    "default_reasoning_effort": "medium",
    "default_extract_json": true,
    "auto_json_response": true,

    "token_counting_model": "gpt-3.5-turbo-16k-0613",
    "token_slice_limit": 14000,

    "embedding_max_tokens": 8191,

    "max_concurrent_queries": 10,
    "max_queue_size": 100
}
